-- ‚úÖ Glow ESP for Chest & Monster (Orc/Goblin)
local function EnableGlowESP()
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Model") and not obj:FindFirstChild("Highlight") then
            local name = obj.Name:lower()
            if name:find("chest") or name:find("orc") or name:find("goblin") then
                local hl = Instance.new("Highlight")
                hl.Name             = "Highlight"
                hl.Adornee          = obj
                hl.FillColor        = name:find("chest") and Color3.fromRGB(255, 215, 0) or Color3.fromRGB(255, 0, 0)
                hl.OutlineColor     = Color3.new(1, 1, 1)
                hl.FillTransparency = 0.25
                hl.Parent           = obj
            end
        end
    end
end

local function DisableGlowESP()
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Model") then
            local hl = obj:FindFirstChild("Highlight")
            if hl then hl:Destroy() end
        end
    end
end

-- ‚úÖ Toggle in Misc tab
local MiscTab = Window:CreateTab("üé≤ Misc", nil)
local ESPToggle = MainTab:CreateToggle({
    Name         = "ESP: Chest & Monster",
    CurrentValue = false,
    Flag         = "Toggle_ESPChestMonster",
    Callback     = function(enabled)
        if enabled then
            EnableGlowESP()
            print("ESP enabled")
        else
            DisableGlowESP()
            print("ESP disabled")
        end
    end,
})
